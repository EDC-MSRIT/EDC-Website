  <script>
    $(function () {
      $(document).scroll(function () {
        var $nav = $("#mainNavbar");
        $nav.toggleClass("scrolled", $(this).scrollTop() > $nav.height());
      });
    });

    // Prealoder---------------------------------------------------------------------------------------------------------------------------------

    function delayNavbar() {
      $("#EDCNav").css({
        opacity: "1",
        transform: "translateX(0)"
      });
    }

    function prealoader() {
      if ($("#preloader").length) {
        $("body").delay(350).css({
          "overflow-y": "auto"
        }); // will enable the verital scroll on body
        $("#mainPage").css({
          "visibility": "initial"
        });
        $("#preloader").css({"display" : "none"}); // will fade out the loader container
        
      }
    }

    // Window load function
    jQuery(window).on("load", function () {
      (function ($) {
        prealoader();
        delayNavbar();
      })(jQuery);
    });
  </script>

